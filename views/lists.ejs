<%- include("header") -%>
<div class="box" id="heading">
  <h1><%= listTitle %></h1>
</div>

<div class="box">
  <% newListItems.forEach(function(item) { %>
  <form>
    <div class="item">
      <input type="checkbox" />
      <p><%= item.name %></p>
    </div>
  </form>
  <% }) %>

  <form action="/" class="item" method="post">
    <input
      type="text"
      name="newItem"
      placeholder="newItem"
      autocomplete="off"
    />
    <button type="submit" name="list" value="<%" ="listTitle" %>>+</button>
  </form>
</div>
<%- include("footer") -%>
